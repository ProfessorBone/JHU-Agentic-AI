# Environment Variables for Agentic AI Project
# Copy this file to .env and add your actual API keys

# Great Learning OpenAI Proxy Configuration (for JHU program)
OPENAI_API_KEY=gl-your_great_learning_token_here
OPENAI_API_BASE=https://aibe.mygreatlearning.com/openai/v1

# Alternative names for compatibility
API_KEY=gl-your_great_learning_token_here

# Hugging Face Configuration
HUGGINGFACE_API_TOKEN=hf_your_huggingface_token_here
HF_TOKEN=hf_your_huggingface_token_here

# Other AI Service Keys (optional)
ANTHROPIC_API_KEY=your_anthropic_key_here

# Project Settings
PROJECT_NAME=JHU-Agentic-AI
ENVIRONMENT=development

# Logging
LOG_LEVEL=INFO